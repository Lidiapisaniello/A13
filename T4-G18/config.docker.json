{
    "postgresUrl": "postgresql://t4_service:t4_password@t4-postgres_db:5432/t4_database",
    "listenAddress": "0.0.0.0:8084",
    "apiPrefix": "/",
    "dataPath": "data",
    "enableSwagger": false,
    "rateLimiting": {
        "enabled": false,
        "burst": 4,
        "maxRate": 2
    },
    "authentication": {
        "enabled": false,
        "headerKey": "Authorization",
        "authEndpoint": "http://auth-service/auth",
        "method": "POST"
    }
}
